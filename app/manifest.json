{
  "name": "japanese-search",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Activeにしている間、Google検索結果を日本語ページのみに限定します。",
  "background": { "page": "background/background.html" },
  "browser_action" :
  {
    "default_icon" : "icon/icon-19-off.png",
    "default_title": "japanese-search"
  },
  "permissions" : [
    "tabs"
  ],
  "icons" : {
    "128" : "icon/icon-128.png"
  }
}
